{
  "name": "express-base",
  "version": "1.0.0",
  "scripts": {
    "build": "rimraf dist & npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npm run watch-ts\" \"nodemon -q dist/index.js\"",
    "watch-ts": "tsc --watch",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.2.5",
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
